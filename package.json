{
  "name": "benefitiary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/*.test.{ts,tsx} --exclude src/**/*integration*.test.{ts,tsx} --exclude src/**/*e2e*.test.{ts,tsx} --exclude src/**/*performance*.test.{ts,tsx}",
    "test:integration": "vitest run src/**/*integration*.test.{ts,tsx}",
    "test:e2e": "vitest run src/**/*e2e*.test.{ts,tsx}",
    "test:performance": "vitest run src/**/*performance*.test.{ts,tsx}",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=junit --reporter=json",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
